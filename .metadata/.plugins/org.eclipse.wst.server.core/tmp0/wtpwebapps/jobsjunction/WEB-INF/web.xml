<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 

<web-app>
	
	<servlet>
		<servlet-name>dispatcher</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>servelet</servlet-name>
		<display-name>servelet</display-name>
		<description></description>
		<servlet-class>sessionSettings.servelet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet</servlet-name>
		<display-name>Servlet</display-name>
		<description></description>
		<servlet-class>sessionSettings.Servlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>dispatcher</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>servelet</servlet-name>
		<url-pattern>/servelet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Servlet</servlet-name>
		<url-pattern>/Servlet</url-pattern>
	</servlet-mapping>
	
</web-app>
